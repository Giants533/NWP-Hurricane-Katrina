'clear'
'open test2.ctl'
'set dbuff on'
t = 1
'set gxout shaded'
'clear'
while (t <= 17)
  'set t 't
  'draw title Pressure (hPa)'
  'd pressure'  
  'cbar'
  'swap'
  'printim pressure/NestBiid2Pressure't'.png png'
  'clear'
  t = t + 1
endwhile


t = 1
'set gxout shaded'
'clear'
while (t <= 17)
  'set t 't
  'draw title Perturbation potential temperature (theta-t0) (K)'
  'd T'  
  'cbar'
  'swap'
  'printim temperature/NestBiid2Temperature't'.png png'
  'clear'
  t = t + 1
endwhile

t = 1
'set gxout shaded'
'set csmooth on'
'clear'
while (t <= 17)
  'set t 't
  'draw title Vorticity'
  'd hcurl(u,v)'  
  'cbar'
  'swap'
  'printim vorticity/NestBiid2Vorticity't'.png png'
  'clear'
  t = t + 1
endwhile

t = 1
'set gxout shaded'
'set csmooth on'
'clear'
while (t <= 17)
  'set t 't
  'draw title Wind modulus (m/s)'
  'd mag(u,v)'  
  'cbar'
  'swap'
  'printim windmod/NestBiid2windmod't'.png png'
  'clear'
  t = t + 1
endwhile

t = 1
'set gxout shaded'
'set csmooth on'
'clear'
while (t <= 17)
  'set t 't
  'draw title Wind direction'
  'd u;v'  
  'swap'
  'printim winddir/NestBiid2winddir't'.png png'
  'clear'
  t = t + 1
endwhile
'clear'